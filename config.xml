<?xml version='1.0' encoding='utf-8'?>
<widget xmlns="http://www.w3.org/ns/widgets"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.w3.org/ns/widgets"
        id="org.wevote.cordova"
        version="1.2.8"
        android-versionCode="1.2.8"
        ios-CFBundleVersion="1.2.8.8">
    <name>We Vote</name>
    <description>
        We Vote Cordova wrapper for the We Vote WebApp root
    </description>
    <version></version>
    <content src="index.html" />
    <access allows-arbitrary-loads-for-media="true" allows-arbitrary-loads-in-web-content="true" allows-local-networking="true" origin="*" />
    <access origin="about:*" />
    <access origin="*://*.facebook.com/*" />
    <access origin="*://*.google.com/*" />
    <access origin="*://*.youtube.com/*" />
    <allow-navigation href="about:*" />
    <allow-navigation href="data:*" />
    <allow-navigation href="*://*.youtube.com/*" />
    <allow-navigation href="*://*.facebook.com/*" />
    <allow-navigation href="*://*.vimeo.com/*" />
    <allow-navigation href="*://*.google.com/*" />
    <allow-navigation href="*://*.googlevideo.com/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="wevotetwitterscheme:" />
    <preference name="AllowedSchemes" value="mycoolapp,wevotetwitterscheme" />
    <platform name="android">
        <preference name="android-manifest/@android:versionName" value="1.5.2.0" />
        <allow-intent href="market:*" />
        <splash density="land-ldpi" src="res/screen/android/screen-ldpi-landscape.png" />
        <splash density="land-mdpi" src="res/screen/android/screen-mdpi-landscape.png" />
        <splash density="land-hdpi" src="res/screen/android/screen-hdpi-landscape.png" />
        <splash density="land-xhdpi" src="res/screen/android/screen-xdpi-landscape.png" />
        <splash density="land-xxhdpi" src="res/screen/android/screen-xxhdpi-landscape.png" />
        <splash density="land-xxxhdpi" src="res/screen/android/screen-xxxhdpi-landscape.png" />
        <splash density="port-ldpi" src="res/screen/android/screen-ldpi-portrait.png" />
        <splash density="port-mdpi" src="res/screen/android/screen-mdpi-portrait.png" />
        <splash density="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
        <splash density="port-xhdpi" src="res/screen/android/screen-xdpi-portrait.png" />
        <splash density="port-xxhdpi" src="res/screen/android/screen-xxhdpi-portrait.png" />
        <splash density="port-xxxhdpi" src="res/screen/android/screen-xxxhdpi-portrait.png" />
        <icon src="res/icon/android/hdpi/ic_launcher.png" density="hdpi" />
        <icon src="res/icon/android/ldpi/ic_launcher.png" density="ldpi" />
        <icon src="res/icon/android/mdpi/ic_launcher.png" density="mdpi" />
        <icon src="res/icon/android/xhdpi/ic_launcher.png" density="xhdpi" />
        <icon src="res/icon/android/xxhdpi/ic_launcher.png" density="xxhdpi" />
        <icon src="res/icon/android/xxxhdpi/ic_launcher.png" density="xxxhdpi" />
        <preference name="android-minSdkVersion" value="19" />
        <preference name="android-targetSdkVersion" value="28" />
    </platform>
    <platform name="ios">
        <config-file parent="CFBundleShortVersionString" target="*-Info.plist" mode="replace">
            <string>12.3.4</string>
        </config-file>
        <config-file parent="CFBundleVersion" target="*-Info.plist" mode="replace">
            <string>5.6.7</string>
        </config-file>
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="FadeSplashScreenDuration" value="0" />
        <preference name="AllowInlineMediaPlayback" value="false" />
        <preference name="BackupWebStorage" value="local" />
        <preference name="DisallowOverscroll" value="true" />
        <preference name="EnableViewportScale" value="false" />
        <preference name="KeyboardDisplayRequiresUserAction" value="true" />
        <preference name="MediaTypesRequiringUserActionForPlayback" value="false" />
        <preference name="SuppressesIncrementalRendering" value="false" />
        <preference name="SuppressesLongPressGesture" value="false" />
        <preference name="Suppresses3DTouchGesture" value="false" />
        <preference name="GapBetweenPages" value="0" />
        <preference name="PageLength" value="0" />
        <preference name="PaginationBreakingMode" value="page" />
        <preference name="PaginationMode" value="unpaginated" />
        <preference name="StatusBarOverlaysWebView" value="true" />
        <preference name="StatusBarStyle" value="lightcontent" />
        <preference name="SplashScreen" value="screen" />
        <preference name="SplashScreenDelay" value="6000" />
        <preference name="SplashScreenSpinnerColor" value="blue" />
        <preference name="KeyboardDisplayRequiresUserAction" value="false" />
        <config-file parent="FacebookAutoLogAppEventsEnabled" target="*-Info.plist">
            <false />
        </config-file>
        <config-file parent="FacebookAdvertiserIDCollectionEnabled" target="*-Info.plist">
            <false />
        </config-file>
        <splash src="res/screen/ios/June2020LaunchScreen.png" />
        <splash width="1125" height="2436" src="res/screen/ios/Launch1125x2436.png" />
        <splash width="2436" height="1125" src="res/screen/ios/Launch2436x1125.png" />
        <splash width="2208" height="1242" src="res/screen/ios/launchLandscape2208x1242.png" />
        <splash width="1242" height="2208" src="res/screen/ios/LaunchPortrait1242x2208.png" />
        <splash width="750" height="1334" src="res/screen/ios/LaunchPortrait750x1334.png" />
        <splash width="640" height="960" src="res/screen/ios/LaunchPortrait640x960.png" />
        <splash width="640" height="1136" src="res/screen/ios/LaunchPortrait640x1136.png" />
        <splash width="640" height="1024" src="res/screen/ios/LaunchPortrait768x1024.png" />
        <splash width="768" height="1024" src="res/screen/ios/LaunchPortrait768x1024.png" />
        <splash width="414" height="736" src="res/screen/ios/Launch414x736.png" />
        <splash width="1536" height="2048" src="res/screen/ios/LaunchPortrait1536x2048.png" />
        <splash width="1024" height="768" src="res/screen/ios/LaunchLandscape1024x768.png" />
        <splash width="2048" height="1536" src="res/screen/ios/LaunchLandscape2048x1536.png" />
        <splash width="320" height="480" src="res/screen/ios/LaunchPortrait320x480.png" />
        <splash width="768" height="1004" src="res/screen/ios/LaunchPortraint768x1004.png" />
        <splash width="1536" height="2008" src="res/screen/ios/LaunchPortrait1536x2008.png" />
        <splash height="1136" src="res/screen/ios/June2020LaunchScreen.png" width="640" />
        <splash height="1334" src="res/screen/ios/June2020LaunchScreen.png" width="750" />
        <splash height="2208" src="res/screen/ios/June2020LaunchScreen.png" width="1242" />
        <splash height="2048" src="res/screen/ios/June2020LaunchScreen.png" width="1536" />
        <splash height="1024" src="res/screen/ios/June2020LaunchScreen.png" width="768" />
        <splash height="960" src="res/screen/ios/June2020LaunchScreen.png" width="640" />
        <splash height="480" src="res/screen/ios/June2020LaunchScreen.png" width="320" />
        <icon height="20" src="res/icon/ios/icon_20pt.png" width="20" />
        <icon height="29" src="res/icon/ios/icon_29pt.png" width="29" />
        <icon height="40" src="res/icon/ios/icon_20pt@2x.png" width="40" />
        <icon height="50" src="res/icon/ios/icon_50pt.png" width="50" />
        <icon height="57" src="res/icon/ios/icon_57pt.png" width="57" />
        <icon height="58" src="res/icon/ios/icon_29pt@2x.png" width="58" />
        <icon height="60" src="res/icon/ios/icon_20pt@3x.png" width="60" />
        <icon height="72" src="res/icon/ios/icon_72pt.png" width="72" />
        <icon height="76" src="res/icon/ios/icon_76pt.png" width="76" />
